{
  "expo": {
    "name": "Death Tracker",
    "slug": "death-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "resizeMode": "cover",
      "backgroundColor": "#030303",
      "image": "./assets/death-tracker-splash2.png"
    },
    "icon": "./assets/death-counter-adaptivelogo3.png",
    "android": {
      "package": "com.amalsraj.deathtracker",
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/death-counter-adaptivelogo3.png"
      }
    }
  },
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "assetBundlePatterns": [
    "**/*"
  ],
  "ios": {
    "supportsTablet": true
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/death-counter-adaptivelogo3.png",
      "backgroundColor": "#FFFFFF"
    },
    "package": "com.amalsraj.deathtracker"
  },
  "web": {}
}